{
    "segment_data": {
        "powerline.segments.common.net.network_load": {
            "args": {
                "recv_format": "⬇ ",
                "sent_format": "⬆ ",
                "recv_max": 7800000.0,
                "sent_max": 1500000.0,
                "si_prefix": false
            }
        },
        "powerline.segments.common.sys.cpu_load_percent": {
            "args": {
                "format": "{0:.0f}%"
            }
        },
        "powerline.segments.common.sys.system_load_compact": {
            "args": {
                "threshold_good": 0.05,
                "threshold_bad": 0.95,
                "track_cpu_count": true
            }
        },
        "powerline.segments.common.bat.battery": {
            "args": {
                "gamify": false
            }
        }
    }
}
